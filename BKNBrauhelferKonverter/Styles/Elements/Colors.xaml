<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


    <Color x:Key="MenuBackground">#FF999999</Color>
    <Color x:Key="MenuIcon">#FF303030</Color>


    <SolidColorBrush x:Key="MenuBackgroundBrush" Color="{StaticResource MenuBackground}" />
    <SolidColorBrush x:Key="MenuIconBrush" Color="{StaticResource MenuIcon}" />



</ResourceDictionary>